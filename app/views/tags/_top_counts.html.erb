<div class="card">
  <div class="card-body">
    <h5 class="card-title">Top Encounter Tags</h5>
    <% tags.each do |tag| %>
    <p class="card-text"><%= link_to tag.name, tag_path(tag) %><span class="ml-2 badge badge-pill badge-primary"><%= tag.tag_count %></span></p>
    <% end %>
  </div>
</div>
