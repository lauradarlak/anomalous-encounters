<div class="py-3 my-3" style="background: #246499;">
  <h2 class="display-6 text-center mb-0 text-white">All Encounter Tags</h2>
</div>
<div class="card">
  <div class="card-body">
    <% @tags.each do |tag| %>
    <p class="card-text"><%= link_to tag.name, tag_path(tag.name.parameterize) %></p>
    <% end %>
  </div>
</div>
