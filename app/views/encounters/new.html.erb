<h1>Encounters#new</h1>

<div class="container">
  <h2>Add a new encounter</h2>
  <%= form_for @encounter, url: user_encounters_path do |f| %>

    <%= render :partial => 'form', :locals=>{:f=>f} %>

    <%= f.submit "Submit Encounter", class: "btn btn-primary btn-round btn-lg" %>

    <% end %>
</div>
